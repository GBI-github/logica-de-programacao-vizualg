Algoritmo "exercicio pratico"

Var
       ano, anoat, idade:inteiro

Inicio
      escreval("em que ano estamos ")
      leia(anoat)
      escreval("em que ano voce nasceu")
      leia (ano)
      idade <- anoat - ano
      se (idade >= 18) entao
         escreva("Voce e maior de idade")
      senao
         escreva("voce e menor de idade")
      fimse

Fimalgoritmo